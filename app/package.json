    "@types/jest": "^25.2.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "jest": "^25.3.0",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "sinon": "^9.0.1",
    "source-map-support": "^0.5.16",
    "ts-jest": "^25.3.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
