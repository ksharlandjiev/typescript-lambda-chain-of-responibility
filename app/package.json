  "scripts": {
    "test": "nyc mocha -r ts-node/register ./src/__tests__/unit/*.ts",
    "test:clean": "mocha -r ts-node/register ./src/__tests__/unit/*.ts",
    "test:lcov": "nyc --reporter=lcov npm run test",
    "test:html": "nyc --reporter=html npm run test",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint --fix -p tsconfig.json -c tslint.json",
    "watch": "webpack-cli src/app.ts -c webpack.config.js -o build/app.js -w",
    "webpack": "webpack-cli src/app.ts -c webpack.config.js -o build/app.js"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
